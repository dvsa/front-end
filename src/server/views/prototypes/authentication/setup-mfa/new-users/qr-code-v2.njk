{% extends 'layouts/default.layout.njk' %}

{% set pageTitle = 'Create an Account' %}

{% block content %}

  {% call components.gridRow() %}

    {% call components.columnFull() %}
      
      {{ components.pageHeading(heading='Scan and check for your 6 digit access code', secondary='Set up your device') }}

      <div class="text u-clearfix">
      {{ components.paragraph(text='1. Scan this QR code using the app:', lead=true) }}
            
               
                    <img style="width: 200px;" src="data:image/png;base64," alt="qr code">
                    <details>
                        <summary style="margin-bottom: 15px;"><span class="summary">I can’t scan the QR code</span></summary>
                        <div class="panel panel-border-narrow">
                            <p>Choose ‘enter code manually’, then enter this key:</p>
                            <h3 class="heading-small" style="font-weight: normal">H5XU ENLO MFUG IYLO<pre id="tpas_token_secret"></pre></h3>
                        </div>
                    </details>

                {{ components.paragraph(text='2.	Check the Authy authenticator app', lead=true) }}</li>
            <p>Enter the  6 digit access code. The code refreshes every 30 seconds.</p>
            {{ components.field(label='Code in app', group=true, width=75) }}
      </div>


      <div class="u-margin-top-30 u-margin-bottom-30">
        {{ components.button(text='Continue', url='/prototypes/authentication/setup-mfa/new-users/app-confirmation-v2') }}
      </div>
     
    {%- endcall %}

  {%- endcall %}
  
{% endblock %}