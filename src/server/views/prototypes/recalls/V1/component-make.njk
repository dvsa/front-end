{% extends 'layouts/mts-without-container.layout.njk' %}

{% set pageTitle = 'homepage' %}

{% set pageBreadcrumbItems = [
  { text: 'Home', url: '/prototypes' },
  { text: 'MOT test results', url: '/prototypes/mot-test' },
  { text: 'Brake test' }
] %}

{% block content %}


<main id="content">

   
    <a href="/prototypes/recalls/V1/type-of-recall" class="link-back">Back</a> 
                      

    <div class="row contents">
        <div class="col-lg-12">

    <div class="grid-row">
    <div class="column-three-quarters">
        <div class="form-group" style="margin-top:20px">
            <label class="heading-large heading heading--page-heading heading--small-margin-bottom" for="select-box" >
                What is the component make?
            </label>
            <select class="form-control" id="select-box" name="select-box" style="margin-top:20px">
                <option value="zero">Choose a Make</option>
                <option value="one">Arctic Cat</option>
                <option value="two">Autoglass Ltd</option>
                <option value="four">Beason Cooke</option>
                <option value="five">Charles Pugh (Glass) Ltd</option>
                <option value="six">Dunlop</option>
                <option value="seven">European Friction Ind</option>
                <option value="eight">Goodyear</option>
                <option value="nine">J &amp; S Component Eng Ltd</option>
                <option value="ten">Michelin</option>
                <option value="eleven">Mitas Tyres Ltd</option>
                <option value="twelve">Mothercare</option>
                <option value="thirteen">Pirelli</option>
                <option value="fourteen">Southern Tyre Co Ltd</option>
                <option value="fifteen">The Fifth Wheel Company Ltd</option>
                <option value="sixteen">Unwin Safety Systems</option>
            </select>
        </div>
        <br>
        <div>
            <a class="button" id="choose-make" href="/prototypes/recalls/V1/component-model" style="margin-bottom:30px">Continue</a>
        </div>

    </div>
</div>            

</main>

{% endblock %}