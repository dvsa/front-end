{% extends 'layouts/mts-without-container.layout.njk' %}

{% set pageTitle = 'homepage' %}

{% set pageBreadcrumbItems = [
  { text: 'Home', url: '/prototypes' },
  { text: 'MOT test results', url: '/prototypes/mot-test' },
  { text: 'Brake test' }
] %}

{% block content %}


<main id="content">

   
    <a href="/prototypes/recalls/V2/vehicle-model" class="link-back">Back</a> 
                      

    <div class="row contents">
        <div class="col-lg-12">

    <div class="grid-row">
    <div class="column-three-quarters">
            <div class="form-group"  style="margin-top:50px;">
            <label class="heading-large heading heading--page-heading heading--small-margin-bottom" for="Year-of-manufactuer" style="margin-top:50px;">
                When was the vehicle manufactured?
                <span class="form-hint" style="margin-top:10px;">For example, 2004</span>
            </label>
            <input class="form-control form-control-1-4" id="Year-of-manufactuer" type="text" name="Year-of-manufactuer" value="" style="margin-top:10px;">
            </div>
        <br>
        <div>
            <a class="button" id="choose-make" href="/prototypes/recalls/V2/results-page" style="margin-bottom:30px">Continue</a>
        </div>

    </div>
</div>            

</main>

{% endblock %}